generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Discs {
  id      Int     @id(map: "id") @default(autoincrement())
  brand   String? @db.VarChar(50)
  name    String? @db.VarChar(50)
  plastic String? @db.VarChar(50)
  speed   Float?  @db.Real
  glide   Float?  @db.Real
  turn    Float?  @db.Real
  fade    Float?  @db.Real
}
